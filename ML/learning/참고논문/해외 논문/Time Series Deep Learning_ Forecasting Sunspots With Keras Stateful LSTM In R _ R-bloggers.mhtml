   =20
    lstm_prediction &lt;- function(data, epochs, ...) {
       =20
        # 5.1.2 Data Setup (MODIFIED)
        df &lt;- data
       =20
        # 5.1.3 Preprocessing
        rec_obj &lt;- recipe(value ~ ., df) %&gt;%
            step_sqrt(value) %&gt;%
            step_center(value) %&gt;%
            step_scale(value) %&gt;%
            prep()
       =20
        df_processed_tbl &lt;- bake(rec_obj, df)
       =20
        center_history &lt;- rec_obj$steps[[2]]$means["value"]
        scale_history  &lt;- rec_obj$steps[[3]]$sds["value"]
       =20
        # 5.1.4 LSTM Plan
        lag_setting  &lt;- 120 # =3D nrow(df_tst)
        batch_size   &lt;- 40
        train_length &lt;- 440
        tsteps       &lt;- 1
        epochs       &lt;- epochs
       =20
        # 5.1.5 Train Setup (MODIFIED)
        lag_train_tbl &lt;- df_processed_tbl %&gt;%
            mutate(value_lag =3D lag(value, n =3D lag_setting)) %&gt;%
            filter(!is.na(value_lag)) %&gt;%
            tail(train_length)
       =20
        x_train_vec &lt;- lag_train_tbl$value_lag
        x_train_arr &lt;- array(data =3D x_train_vec, dim =3D c(length(x_tr=
ain_vec), 1, 1))
       =20
        y_train_vec &lt;- lag_train_tbl$value
        y_train_arr &lt;- array(data =3D y_train_vec, dim =3D c(length(y_tr=
ain_vec), 1))
       =20
        x_test_vec &lt;- y_train_vec %&gt;% tail(lag_setting)
        x_test_arr &lt;- array(data =3D x_test_vec, dim =3D c(length(x_test=
_vec), 1, 1))
               =20
        # 5.1.6 LSTM Model
        model &lt;- keras_model_sequential()

        model %&gt;%
            layer_lstm(units            =3D 50,=20
                       input_shape      =3D c(tsteps, 1),=20
                       batch_size       =3D batch_size,
                       return_sequences =3D TRUE,=20
                       stateful         =3D TRUE) %&gt;%=20
            layer_lstm(units            =3D 50,=20
                       return_sequences =3D FALSE,=20
                       stateful         =3D TRUE) %&gt;%=20
            layer_dense(units =3D 1)
       =20
        model %&gt;%=20
            compile(loss =3D 'mae', optimizer =3D 'adam')
       =20
        # 5.1.7 Fitting LSTM
        for (i in 1:epochs) {
            model %&gt;% fit(x          =3D x_train_arr,=20
                          y          =3D y_train_arr,=20
                          batch_size =3D batch_size,
                          epochs     =3D 1,=20
                          verbose    =3D 1,=20
                          shuffle    =3D FALSE)
           =20
            model %&gt;% reset_states()
            cat("Epoch: ", i)
           =20
        }
       =20
        # 5.1.8 Predict and Return Tidy Data (MODIFIED)
        # Make Predictions
        pred_out &lt;- model %&gt;%=20
            predict(x_test_arr, batch_size =3D batch_size) %&gt;%
            .[,1]=20
       =20
        # Make future index using tk_make_future_timeseries()
        idx &lt;- data %&gt;%
            tk_index() %&gt;%
            tk_make_future_timeseries(n_future =3D lag_setting)
       =20
        # Retransform values
        pred_tbl &lt;- tibble(
            index   =3D idx,
            value   =3D (pred_out * scale_history + center_history)^2
        )
       =20
        # Combine actual data with predictions
        tbl_1 &lt;- df %&gt;%
            add_column(key =3D "actual")

        tbl_3 &lt;- pred_tbl %&gt;%
            add_column(key =3D "predict")

        # Create time_bind_rows() to solve dplyr issue
        time_bind_rows &lt;- function(data_1, data_2, index) {
            index_expr &lt;- enquo(index)
            bind_rows(data_1, data_2) %&gt;%
                as_tbl_time(index =3D !! index_expr)
        }

        ret &lt;- list(tbl_1, tbl_3) %&gt;%
            reduce(time_bind_rows, index =3D index) %&gt;%
            arrange(key, index) %&gt;%
            mutate(key =3D as_factor(key))

        return(ret)
       =20
    }
   =20
    safe_lstm &lt;- possibly(lstm_prediction, otherwise =3D NA)
   =20
    safe_lstm(data, epochs, ...)
   =20
